{
  "name": "vizapp-web-embed",
  "version": "0.1.0",
  "private": true,
  "description": "Project that tests the ability to npm install jupyter-js-widgets within an npm project.",
  "license": "BSD-3-Clause",
  "author": "Nicholas Earl",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc && node scripts/copyfiles.js && webpack",
    "clean": "rimraf dist && rimraf built",
    "host": "http-server",
    "lint": "tslint --project tslint.json --format stylish",
    "test": "npm run test:default",
    "test:default": "echo \"No test specified\"",
    "watch": "tsc -w",
    "start": "npm run build && electron ./dist/main.js"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.2.4",
    "@jupyter-widgets/controls": "^1.4.4",
    "@jupyter-widgets/html-manager": "^0.16.1",
    "@jupyter-widgets/jupyterlab-manager": "^0.40.0",
    "@jupyterlab/services": "^3.2.1",
    "@phosphor/widgets": "^1.6.0",
    "bqplot-image-gl": "^0.1.4",
    "codemirror": "^5.47.0",
    "font-awesome": "^4.7.0",
    "http-server": "^0.11.1",
    "jupyter-materialui": "^0.0.1-alpha.0",
    "jupyter-matplotlib": "^0.3.0",
    "plotlywidget": "^0.10.0"
  },
  "devDependencies": {
    "css-loader": "^2.1.1",
    "electron": "^5.0.1",
    "file-loader": "^3.0.1",
    "fs-extra": "^8.0.1",
    "json-loader": "^0.5.7",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "postcss": "^7.0.16",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.4",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "resolutions": {
    "@types/react": "^15.0.16"
  }
}
